<?xml version="1.0" encoding="utf-8"?>
<androidx.preference.PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.preference.PreferenceCategory>
        <androidx.preference.SwitchPreferenceCompat
            android:key="auto_like"
            android:title="@string/biliroaming_auto_like_title" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="save_comment_image"
            android:summary="@string/biliroaming_save_comment_image_summary"
            android:title="@string/biliroaming_save_comment_image_title" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="play_arc_conf"
            android:summary="@string/biliroaming_play_arc_conf_summary"
            android:title="@string/biliroaming_play_arc_conf_title" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="replace_story_video"
            android:summary="@string/biliroaming_replace_story_video_summary"
            android:title="@string/biliroaming_replace_story_video_title" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="disable_story_full"
            android:summary="@string/biliroaming_disable_story_full_summary"
            android:title="@string/biliroaming_disable_story_full_title" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="remove_video_cmd_dms"
            android:summary="@string/biliroaming_remove_video_cmd_dms_summary"
            android:title="@string/biliroaming_remove_video_cmd_dms_title" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="block_dm_feedback"
            android:summary="@string/biliroaming_block_dm_feedback_summary"
            android:title="@string/biliroaming_block_dm_feedback_title" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="block_fan_guide"
            android:title="@string/biliroaming_block_fan_guide_title" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="remove_elec_button"
            android:title="@string/biliroaming_remove_elec_button_title" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="block_live_order"
            android:title="@string/biliroaming_block_live_order_title" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="block_activity_tab"
            android:summary="@string/biliroaming_block_activity_tab_summary"
            android:title="@string/biliroaming_block_activity_tab_title" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="not_lock_orientation"
            android:summary="@string/biliroaming_not_lock_orientation_summary"
            android:title="@string/biliroaming_not_lock_orientation_title" />
        <androidx.preference.Preference
            android:fragment="app.revanced.bilibili.settings.fragments.VideoDetailFilterFragment"
            android:key="fragment_video_relate"
            android:title="@string/biliroaming_video_relate_title" />
        <androidx.preference.Preference
            android:fragment="app.revanced.bilibili.settings.fragments.FilterStoryFragment"
            android:key="fragment_filter_story"
            android:title="@string/biliroaming_filter_story_title" />
        <app.revanced.bilibili.widget.RadioGroupEntrancePreference
            android:fragment="app.revanced.bilibili.settings.fragments.StoryUIStyleFragment"
            android:key="story_ui_style"
            android:persistent="true"
            android:title="@string/biliroaming_story_ui_style_title"
            app:migration_settings_type="none"
            app:radioDefaultValue="0"
            app:radioEntries="@array/biliroaming_story_ui_style_entries"
            app:radioEntryValues="@array/biliroaming_story_ui_style_values" />
        <androidx.preference.Preference
            android:fragment="app.revanced.bilibili.settings.fragments.ExternalDownloaderFragment"
            android:key="fragment_external_downloader"
            android:title="@string/biliroaming_external_downloader_title" />
    </androidx.preference.PreferenceCategory>
    <androidx.preference.PreferenceCategory
        android:key="category_comment"
        android:layout="@layout/bili_app_layout_preference_category_title"
        android:title="@string/biliroaming_video_detail_comment_category_title">
        <androidx.preference.SwitchPreferenceCompat
            android:key="block_word_search"
            android:summary="@string/biliroaming_block_word_search_summary"
            android:title="@string/biliroaming_block_word_search_title" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="block_comment_guide"
            android:summary="@string/biliroaming_block_comment_guide_summary"
            android:title="@string/biliroaming_block_comment_guide_title" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="block_comment_feedback"
            android:title="@string/biliroaming_block_comment_feedback_title" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="block_video_comment"
            android:title="@string/biliroaming_block_video_comment_title" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="check_comment"
            android:summary="@string/biliroaming_check_comment_summary"
            android:title="@string/biliroaming_check_comment_title" />
