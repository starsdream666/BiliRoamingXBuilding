            android:key="unlock_gif"
            android:summary="@string/biliroaming_unlock_gif_summary"
            android:title="@string/biliroaming_unlock_gif_title" />
        <androidx.preference.Preference
            android:fragment="app.revanced.bilibili.settings.fragments.FilterCommentFragment"
            android:key="fragment_filter_comment"
            android:title="@string/biliroaming_filter_comment_title" />
    </androidx.preference.PreferenceCategory>
    <androidx.preference.PreferenceCategory
        android:key="category_video_detail_ad"
        android:layout="@layout/bili_app_layout_preference_category_title"
        android:title="@string/biliroaming_video_detail_ad_category_title">
        <androidx.preference.SwitchPreferenceCompat
            android:key="block_up_rcmd_ads"
            android:summary="@string/biliroaming_block_up_rcmd_ads_summary"
            android:title="@string/biliroaming_block_up_rcmd_ads_title" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="block_bangumi_page_ads"
            android:summary="@string/biliroaming_block_bangumi_page_ads_summary"
            android:title="@string/biliroaming_block_bangumi_page_ads_title" />
    </androidx.preference.PreferenceCategory>
    <androidx.preference.PreferenceCategory
        android:key="category_video_detail_other"
        android:layout="@layout/bili_app_layout_preference_category_title"
        android:title="@string/biliroaming_video_detail_other_category_title">
        <androidx.preference.SwitchPreferenceCompat
            android:key="disable_auto_select"
            android:summary="@string/biliroaming_disable_auto_select_summary"
            android:title="@string/biliroaming_disable_auto_select_title" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="disable_auto_subscribe"
            android:summary="@string/biliroaming_disable_auto_subscribe_summary"
            android:title="@string/biliroaming_disable_auto_subscribe_title" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="old_fav"
            android:summary="@string/biliroaming_old_fav_summary"
            android:title="@string/biliroaming_old_fav_title" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="no_colorful_danmaku"
            android:title="@string/biliroaming_no_colorful_danmaku_title" />
        <androidx.preference.SwitchPreferenceCompat
            android:key="fake_not_in_multi_window"
            android:summary="@string/biliroaming_fake_not_in_multi_window_summary"
            android:title="@string/biliroaming_fake_not_in_multi_window_title" />
    </androidx.preference.PreferenceCategory>
</androidx.preference.PreferenceScreen>
